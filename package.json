{
  "name": "blaseball-feed",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist pkg",
    "build": "yarn build:tsc && yarn build:fix",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:fix": "node ./scripts/fix-paths.js",
    "start": "yarn build && node ./dist/cli.js",
    "test": "jest --testPathPattern=tests/.*\\.test\\.ts",
    "package": "yarn build && yarn package:win && yarn package:mac && yarn package:linux",
    "package:win": "yarn pkg -t node14-win-x64 -o pkg/win/blaseball-feed.exe dist/cli.js",
    "package:mac": "yarn pkg -t node14-macos-x64 -o pkg/mac/blaseball-feed dist/cli.js",
    "package:linux": "yarn pkg -t node14-linux-x64 -o pkg/linux/blaseball-feed dist/cli.js"
  },
  "dependencies": {
    "fp-ts": "^2.8.1",
    "rxjs": "^6.6.2",
    "socket.io-client": "^2.3.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/socket.io-client": "^1.4.33",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "pkg": "^4.4.9",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=14.7.0"
  }
}
